---
import Layout from '@/web/components/layout.astro';
import Shortner from '@/web/components/Shortner';
import cv from '@/data';

const { config, sections } = cv();
---

<Layout {...config} sections={sections}>
  <main class="w-full max-w-7xl space-y-4 px-2 py-3 sm:space-y-6 sm:px-8 sm:py-12 lg:space-y-8 lg:py-12">
    <Shortner client:load />
  </main>
</Layout>
